<form bindsubmit="saveSettings" report-submit="true">
<button form-type="submit" class="one-row" hover-class="none"
        wx:for="{{topic_list}}" wx:key="key"
        wx:for-item="topic_info">
<!-- <view > -->
  <image src="{{topic_info.topic_url}}"></image>
  <view class="one-row-right">
    <text space='ensp'>{{topic_info.topic_name}} 第<text style="font-weight:bold;color:{{topic_info.rank<=10?'red':'blue'}};">{{topic_info.rank}}</text>名 </text>
    <text style="font-size:25rpx;" space='ensp'>完成度<text style="font-weight:bold;color:{{topic_info.complete_rate>=50?'red':'blue'}};">{{topic_info.complete_rate}}</text>% 超过<text style="font-weight:bold;color:{{topic_info.higher_rate>=50?'red':'blue'}};">{{topic_info.higher_rate}}</text>%的同学</text>
  </view>
<!-- </view> -->
</button>
</form>